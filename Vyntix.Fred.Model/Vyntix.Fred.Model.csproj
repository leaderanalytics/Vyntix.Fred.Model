<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
    <Authors>Leader Analytics</Authors>
    <Company>Leader Analytics</Company>
    <Product>Vyntix.Fred.Model</Product>
    <Description>Data Transfer Objects for FredClient.</Description>
    <Copyright>2021 Leader Analytics</Copyright>
    <PackageProjectUrl>https://github.com/leaderanalytics/</PackageProjectUrl>
    <PackageIcon>VyntixLogo.png</PackageIcon>
    <RepositoryUrl>https://github.com/leaderanalytics/</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>Leader Analytics LeaderAnalytics Vyntix FRED</PackageTags>
    <PackageReleaseNotes>See Repository</PackageReleaseNotes>
    <Version>1.0.0-beta.1</Version>
    <AssemblyVersion>1.0.0.0</AssemblyVersion>
    <FileVersion>1.0.0.0</FileVersion>
    <PackageId>LeaderAnalytics.Vyntix.Fred.Model</PackageId>
    <PackageLicenseFile>LICENSE.md</PackageLicenseFile>
  </PropertyGroup>

  <PropertyGroup>
    <RootNamespace>LeaderAnalytics.Vyntix.Fred.Model</RootNamespace>
    <AssemblyName>LeaderAnalytics.Vyntix.Fred.Model</AssemblyName>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="LeaderAnalytics.Core.Serialization" Version="1.0.2" />
  </ItemGroup>
  
  <ItemGroup>
    <None Include="..\VyntixLogo.png" Pack="true" PackagePath="\" />
    <None Include="..\LICENSE.md" Pack="true" PackagePath="\" />
  </ItemGroup>
</Project>
